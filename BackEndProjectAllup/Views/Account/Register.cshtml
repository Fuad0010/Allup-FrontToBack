@model RegisterVM

@{
    Layout = null;
}

<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title> Free Dental Medical Hospital Website Template | Smarteyeapps.com</title>

    <link rel="shortcut icon" href="~/assets/logreg/images/fav.jpg">
    <link rel="stylesheet" href="~/assets/logreg/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/logreg/css/fontawsom-all.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/logreg/css/style.css" />
</head>

    <body class="h-100">
        <div class="container-fluid full-bg h-100">
           <div class="container h-100">
               <div class="row no-margin h-100">
                   <div class="bg-layer d-flex col-md-8">
                    <form method="post">
                        <div class="login-box row">
                            <h3>User Registeration</h3>
                               <label asp-for="Fullname"></label>
                            <div class="input-group mb-3">
                              <div class="input-group-prepend">
                              </div>
                              <input asp-for="Fullname" type="text" class="form-control" placeholder="Full Name">
                              <span class="text-danger" asp-validation-for="Fullname"></span>    
                            </div>
                               <label asp-for="Name"></label>
                            <div class="input-group mb-3">
                              <div class="input-group-prepend">
                              </div>
                              <input asp-for="Name" type="text" class="form-control" placeholder="Name">
                              <span class="text-danger" asp-validation-for="Name"></span>         
                            </div>
                                  <label asp-for="Email"></label>
                            <div class="input-group mb-3">
                              <div class="input-group-prepend">
                              </div>
                              <input asp-for="Email" type="text" class="form-control" placeholder="Email">
                              <span class="text-danger" asp-validation-for="Email"></span>         
                            </div>
                               <label asp-for="Password"></label>
                             <div class="input-group mb-3">
                              <div class="input-group-prepend">
                              </div>
                              <input asp-for="Password" type="text" class="form-control" placeholder="Password">
                              <span class="text-danger" asp-validation-for="Password"></span>         
                            </div>
                                  <label asp-for="RepeatPassword"></label>
                            <div class="input-group mb-3">
                              <div class="input-group-prepend">
                              </div>
                              <input asp-for="RepeatPassword" type="text" class="form-control" placeholder="Repeat Password">
                              <span class="text-danger" asp-validation-for="RepeatPassword"></span>         
                            </div>
                           
                            
                            <div class="text-danger" asp-validation-summary="ModelOnly"></div>
                            
                            <button type="submit" class="btn btn-success">Click to Register</button>
                            
                            <p class="no-c">Already have Account? <a asp-area="" asp-controller="Account" asp-action="Login">Sign In</a></p>
                        </div>
                    </form>
                    </div>
               </div>
              
               <div class="foter-credit">
                  <a href="https://smarteyeapps.com/">Designed by : Smarteyeapps.com</a>  
               </div>
           </div>
            
        </div>
    </body>

<script src="~/assets/logreg/js/jquery-3.2.1.min.js"></script>
<script src="~/assets/logreg/js/popper.min.js"></script>
<script src="~/assets/logreg/js/bootstrap.min.js"></script>        
<script src="~/assets/logreg/js/script.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>

</html>























